{
  "kind": "collectionType",
  "collectionName": "case_studies",
  "info": {
    "singularName": "case-study",
    "pluralName": "case-studies",
    "displayName": "Case Study",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "heroSection": {
      "type": "component",
      "repeatable": false,
      "component": "shared.card"
    },
    "problemSection": {
      "type": "component",
      "repeatable": false,
      "component": "shared.callout"
    },
    "solutionSection": {
      "type": "component",
      "repeatable": false,
      "component": "shared.solution"
    },
    "sidebarData": {
      "type": "component",
      "repeatable": true,
      "component": "shared.card"
    },
    "relatedCaseStudies": {
      "type": "component",
      "repeatable": true,
      "component": "shared.card"
    },
    "industries": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::industry.industry",
      "mappedBy": "case_study"
    }
  }
}
